show DATABASES;
SHOW TABLES;

SELECT IL.NAME, I.SUMMARY, I.DESCRIPTION
FROM issues_lists IL JOIN task.issues I on IL.ID = I.ISSUES_LIST_ID
GROUP BY IL.NAME, I.SUMMARY, I.DESCRIPTION
HAVING COUNT(*) >= 1
ORDER BY IL.NAME;

SELECT U.FIRSTNAME, U.LASTNAME, COUNT(*) AS POSTS_NUMBER
FROM users U JOIN task.issues I on U.ID = I.USER_ID_ASSIGNEDTO
GROUP BY U.FIRSTNAME, U.LASTNAME
HAVING COUNT(*) >= 1
ORDER BY U.LASTNAME,U.FIRSTNAME;